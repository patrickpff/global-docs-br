{
  "name": "global-docs-br",
  "version": "0.1.3",
  "description": "Brazilian document validators and formatters for global-docs",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patrickpff/global-docs-br.git"
  },
  "keywords": [
    "global-docs",
    "documents",
    "brazil",
    "cpf",
    "cnpj",
    "rg",
    "ie",
    "oab",
    "cnh",
    "passport",
    "validation",
    "mask"
  ],
  "author": "Patrick Ferreira",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/patrickpff/global-docs-br/issues"
  },
  "homepage": "https://github.com/patrickpff/global-docs-br#readme",
  "dependencies": {
    "global-docs": "^0.1.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@vitest/coverage-v8": "^4.0.16",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
