{
  "name": "global-docs-br",
  "version": "0.1.0",
  "description": "Brazilian document validators and formatters for global-docs",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "exports": {
    ".": "./src/index.js",
    "./documents/cpf": "./dist/documents/cpf/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patrickpff/global-docs-br.git"
  },
  "keywords": [
    "global-docs",
    "documents",
    "brazil",
    "cau",
    "cnh",
    "cnpj",
    "cpf",
    "crc",
    "crea",
    "crm",
    "cro",
    "ie",
    "oab",
    "passport",
    "process-cnj",
    "rg",
    "voter-registration",
    "validation",
    "mask"
  ],
  "author": "Patrick Ferreira",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/patrickpff/global-docs-br/issues"
  },
  "homepage": "https://github.com/patrickpff/global-docs-br#readme",
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@vitest/coverage-v8": "^4.0.16",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "vitest": {
    "include": [
      "src/**/*.spec.ts"
    ],
    "exclude": [
      "node_modules",
      "dist"
    ]
  },
  "dependencies": {
    "global-docs": "^0.1.0"
  }
}
